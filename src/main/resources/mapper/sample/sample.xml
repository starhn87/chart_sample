<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd">
<mapper namespace="com.vaiv.chart.rest.mapper.SampleMapper">
    <select id="getSample" resultType="com.vaiv.chart.rest.domain.Sample">
        SELECT
            *
        FROM
            SAMPLE
        WHERE
            ID = 1
    </select>
</mapper>
